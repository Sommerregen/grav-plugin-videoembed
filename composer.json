{
    "name": "maximkou/grav-plugin-youtube",
    "description": "Youtube plugin for Grav CMS",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Hodyrev",
            "email": "maximkou@gmail.com"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/getgrav/grav.git",
            "type": "git"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "twig/twig": "~1.16",
        "erusev/parsedown-extra": "dev-master",
        "symfony/yaml": "~2.5",
        "symfony/console": "~2.5",
        "symfony/event-dispatcher": "~2.5",
        "doctrine/cache": "~1.3",
        "tracy/tracy": "~2.2",
        "gregwar/image": "~2.0",
        "ircmaxell/password-compat": "1.0.*",
        "mrclay/minify": "~2.2",
        "donatj/phpuseragentparser": "dev-master",
        "pimple/pimple": "~3.0",
        "getgrav/grav": "dev-master",
        "squizlabs/php_codesniffer": "1.*",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Maximkou\\GravPluginYoutube\\Tests\\": "tests/",
            "Grav\\": "vendor/getgrav/grav/system/src/Grav"
        },
        "files": ["youtube.php"]
    }
}